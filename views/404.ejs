    <!-- Custom 404 Script -->
    <script>
        $(document).ready(function() {
            // Animate error message on load
            $('.error-title').addClass('wow fadeInDown');
            $('.error-text').addClass('wow fadeInUp');
            new WOW().init();

            // Random fun message on button hover
            const messages = [
                "Sail back to safety!",
                "Find your property now!",
                "Chart a new course!",
                "Captain your rentals!"
            ];
            $('.hamilton-btn.error a').hover(function() {
                const randomMsg = messages[Math.floor(Math.random() * messages.length)];
                $(this).text(randomMsg);
            }, function() {
                $(this).text("Return to Harbor");
            });
        });
    </script>
