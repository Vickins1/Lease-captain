    <div class="container mt-5">
        <h3 class="text-center"><strong>Users Management</strong></h3>

        <!-- Error Message -->
        <% if (error && error.length> 0) { %>
            <div class="alert alert-danger text-center" role="alert" style="font-size: 0.9rem;">
                <%= error %>
            </div>
            <% } %>

                <!-- Success Message -->
                <% if (success && success.length> 0) { %>
                    <div class="alert alert-success text-center" role="alert" style="font-size: 0.9rem;">
                        <%= success %>
                    </div>
                    <% } %>

                        <style>
                            @media (max-width: 576px) {
                                .alert {
                                    font-size: 0.8rem;
                                }
                            }
                        </style>

                        <!-- Button to trigger modal -->
                        <button type="button" class="btn btn-dark mb-3" style="background-color: #003366;"
                            data-toggle="modal" data-target="#addUserModal">
                            Add New User
                        </button>

                        <div class="container mt-5">
                            <div class="table-responsive">
                                <table class="table table-bordered table-hover text-center">
                                    <thead class="thead-light">
                                        <tr>
                                            <th>Username</th>
                                            <th>Email</th>
                                            <th>Roles</th>
                                            <th>Assign Role</th>
                                            <th>Delete User</th> <!-- Add a column for deleting a user -->
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% users.forEach(user=> { %>
                                            <tr>
                                                <td>
                                                    <%= user.username %>
                                                </td>
                                                <td>
                                                    <%= user.email %>
                                                </td>
                                                <td>
                                                    <% if (user.roles && user.roles.length> 0) { %>
                                                        <ul class="list-unstyled">
                                                            <% user.roles.forEach(role=> { %>
                                                                <li>
                                                                    <!-- Assign different colors based on role names -->
                                                                    <% if (role.name==='Admin' ) { %>
                                                                        <span class="badge badge-danger">
                                                                            <%= role.name %>
                                                                        </span>
                                                                        <% } else if (role.name==='Manager' ) { %>
                                                                            <span class="badge badge-primary">
                                                                                <%= role.name %>
                                                                            </span>
                                                                            <% } else { %>
                                                                                <span class="badge badge-success">
                                                                                    <%= role.name %>
                                                                                </span>
                                                                                <% } %>
                                                                </li>
                                                                <% }); %>
                                                        </ul>
                                                        <% } else { %>
                                                            <span class="text-muted">No roles assigned</span>
                                                            <% } %>
                                                </td>
                                                <td>
                                                    <!-- Button to trigger the assign role modal -->
                                                    <button class="btn btn-info btn-sm" data-toggle="modal"
                                                        data-target="#assignRoleModal"
                                                        onclick="openRoleModal('<%= user._id %>', '<%= user.username %>')">
                                                        Assign Roles
                                                    </button>
                                                </td>
                                                <td>
                                                    <!-- Button to trigger the delete confirmation modal -->
                                                    <button class="btn btn-danger btn-sm" data-toggle="modal"
                                                        data-target="#deleteUserModal"
                                                        onclick="openDeleteModal('<%= user._id %>', '<%= user.username %>')">
                                                        Delete
                                                    </button>
                                                </td>
                                            </tr>
                                            <% }); %>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <!-- Modal for Assigning Roles -->
                        <div class="modal fade" id="assignRoleModal" tabindex="-1" role="dialog"
                            aria-labelledby="assignRoleModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="assignRoleModalLabel">Assign Roles to <span
                                                id="modalUserName"></span></h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <form id="assignRoleForm" method="POST">
                                        <div class="modal-body">
                                            <div class="form-group">
                                                <label for="role" class="role-label">Roles</label>
                                                <div class="select-wrapper">
                                                    <% roles.forEach(role=> { %>
                                                        <div class="checkbox-wrapper">
                                                            <input type="checkbox" id="role-<%= role._id %>"
                                                                name="roleIds[]" value="<%= role._id %>">
                                                            <label for="role-<%= role._id %>" class="checkbox-label">
                                                                <span class="checkbox-custom"></span>
                                                                <%= role.name %>
                                                            </label>
                                                        </div>
                                                        <% }); %>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-secondary"
                                                data-dismiss="modal">Close</button>
                                            <button type="submit" class="btn btn-dark"
                                                style="background-color: #003366;">Assign Roles</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <script>
                            function openRoleModal(userId, username) {
                                document.getElementById('assignRoleForm').action = '/users/' + userId + '/assign-role';
                                document.getElementById('modalUserName').textContent = username;
                                $('#assignRoleModal').modal('show');
                            }
                            function openRoleModal(userId, username) {
                                document.getElementById('assignRoleForm').action = '/users/' + userId + '/assign-role';
                                document.getElementById('modalUserName').textContent = username;
                                $('#assignRoleModal').modal('show');
                            }

                            function openDeleteModal(userId, username) {
                                document.getElementById('deleteUserForm').action = '/users/' + userId + '/delete';
                                document.getElementById('deleteModalUserName').textContent = username;
                                $('#deleteUserModal').modal('show');
                            }
                        </script>

                        <!-- Modal for Deleting User -->
                        <div class="modal fade" id="deleteUserModal" tabindex="-1" role="dialog"
                            aria-labelledby="deleteUserModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="deleteUserModalLabel">Delete User: <span
                                                id="deleteModalUserName"></span></h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        Are you sure you want to delete this user? This action cannot be undone.
                                    </div>
                                    <div class="modal-footer">
                                        <form id="deleteUserForm" method="POST">
                                            <button type="button" class="btn btn-secondary"
                                                data-dismiss="modal">Cancel</button>
                                            <button type="submit" class="btn btn-danger">Delete</button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>




                        <!-- Modal for Adding a New User -->
                        <div class="modal fade" id="addUserModal" tabindex="-1" role="dialog"
                            aria-labelledby="addUserModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="addUserModalLabel">Add New User</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <form action="/users/create" method="POST">
                                            <div class="form-group">
                                                <label for="username">Username</label>
                                                <input type="text" class="form-control" id="username" name="username"
                                                    required>
                                            </div>
                                            <div class="form-group">
                                                <label for="email">Email</label>
                                                <input type="email" class="form-control" id="email" name="email"
                                                    required>
                                            </div>
                                            <div class="form-group">
                                                <label for="password">Password</label>
                                                <input type="password" class="form-control" id="password"
                                                    name="password" required>
                                            </div>
                                            <div class="form-group">
                                                <label for="role" class="role-label">Roles</label>
                                                <div class="select-wrapper">
                                                    <% roles.forEach(role=> { %>
                                                        <div class="checkbox-wrapper">
                                                            <input type="checkbox" id="role-<%= role._id %>"
                                                                name="roleIds[]" value="<%= role._id %>">
                                                            <label for="role-<%= role._id %>" class="checkbox-label">
                                                                <span class="checkbox-custom"></span>
                                                                <%= role.name %>
                                                            </label>
                                                        </div>
                                                        <% }); %>
                                                </div>
                                            </div>

                                            <style>
                                                .form-group {
                                                    margin-bottom: 1rem;
                                                    text-align: center;
                                                }

                                                .role-label {
                                                    font-weight: bold;
                                                    font-size: 1.1rem;
                                                    color: #003366;
                                                    margin-bottom: 8px;
                                                }

                                                .select-wrapper {
                                                    display: flex;
                                                    flex-direction: column;
                                                    gap: 10px;
                                                    align-items: center;
                                                    margin-top: 8px;
                                                    padding: 8px;
                                                    background-color: #f9f9f9;
                                                    border-radius: 10px;
                                                    box-shadow: 0 3px 8px rgba(0, 0, 0, 0.1);
                                                    max-width: 350px;
                                                    margin: 0 auto;
                                                }

                                                .checkbox-wrapper {
                                                    position: relative;
                                                    display: flex;
                                                    align-items: center;
                                                    cursor: pointer;
                                                    padding: 8px 12px;
                                                    border: 1px solid #ccc;
                                                    border-radius: 15px;
                                                    transition: border-color 0.3s, background-color 0.3s, box-shadow 0.3s;
                                                    width: 100%;
                                                    justify-content: flex-start;
                                                    background-color: #fff;
                                                }

                                                .checkbox-wrapper:hover {
                                                    border-color: #003366;
                                                    background-color: #f0f8ff;
                                                    box-shadow: 0 0 5px rgba(0, 51, 102, 0.2);
                                                }

                                                .checkbox-wrapper input[type="checkbox"] {
                                                    opacity: 0;
                                                    position: absolute;
                                                    left: 0;
                                                    top: 0;
                                                    width: 100%;
                                                    height: 100%;
                                                    cursor: pointer;
                                                    z-index: 1;
                                                }

                                                .checkbox-custom {
                                                    width: 16px;
                                                    height: 16px;
                                                    border: 1px solid #003366;
                                                    border-radius: 50%;
                                                    margin-right: 10px;
                                                    transition: background-color 0.3s, border-color 0.3s;
                                                }

                                                .checkbox-label {
                                                    font-size: 0.9rem;
                                                    color: #333;
                                                    cursor: pointer;
                                                    display: flex;
                                                    align-items: center;
                                                }

                                                .checkbox-wrapper input[type="checkbox"]:checked+.checkbox-label .checkbox-custom {
                                                    background-color: #003366;
                                                    border-color: #003366;
                                                }

                                                @media (max-width: 768px) {
                                                    .checkbox-wrapper {
                                                        padding: 6px;
                                                        font-size: 0.85rem;
                                                    }

                                                    .checkbox-custom {
                                                        width: 14px;
                                                        height: 14px;
                                                    }
                                                }
                                            </style>


                                            <button type="submit" class="btn btn-dark"
                                                style="background-color: #003366;">Add User</button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <style>
                            @media (max-width: 576px) {
                                .modal-dialog {
                                    margin: 1rem;
                                    width: 90%;
                                }

                                .table {
                                    font-size: 0.7rem;
                                }

                                .btn {
                                    margin-bottom: 0.5rem;
                                    font-size: 0.7rem;
                                }

                                .modal-dialog {
                                    margin: 1rem;
                                    max-width: 100%;
