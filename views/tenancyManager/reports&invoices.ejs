        <div class="content-wrapper">
            <section class="content">
                <div class="container-fluid">
                    <!-- Header -->
                    <div class="row mb-4">
                        <div class="col-12 text-center">
                            <header class="dashboard-header">
                                <h1 class="header-title"><i class="fas fa-file-alt mr-2"></i> Reports & Invoices</h1>
                                <p class="header-subtitle">Gain insights and manage finances with precision</p>
                            </header>
                        </div>
                    </div>
        
                    <!-- Tabs -->
                    <ul class="nav nav-tabs custom-tabs mb-4" id="reportsInvoicesTab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="reports-tab" data-toggle="tab" href="#reports" role="tab" aria-controls="reports" aria-selected="true">
                                <i class="fas fa-chart-line mr-2"></i> Reports
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="invoices-tab" data-toggle="tab" href="#invoices" role="tab" aria-controls="invoices" aria-selected="false">
                                <i class="fas fa-file-invoice mr-2"></i> Invoices
                            </a>
                        </li>
                    </ul>
        
                    <!-- Tab Content -->
                    <div class="tab-content" id="reportsInvoicesTabContent">
                        <!-- Reports Section -->
                        <div class="tab-pane fade show active" id="reports" role="tabpanel" aria-labelledby="reports-tab">
                            <div class="card custom-card">
                                <div class="card-header">
                                    <h4><i class="fas fa-chart-pie mr-2"></i> Property Performance Reports</h4>
                                </div>
                                <div class="card-body">
                                    <!-- Messages -->
                                    <% if (error && error.length > 0) { %>
                                        <div class="alert alert-danger animate__animated animate__fadeIn">
                                            <i class="fas fa-exclamation-circle mr-2"></i> <%= error %>
                                        </div>
                                    <% } %>
                                    <% if (success && success.length > 0) { %>
                                        <div class="alert alert-success animate__animated animate__fadeIn">
                                            <i class="fas fa-check-circle mr-2"></i> <%= success %>
                                        </div>
                                    <% } %>
        
                                    <!-- Filter Form -->
                                    <form method="GET" action="/reports-invoices" class="filter-form mb-4">
                                        <div class="row align-items-end">
                                            <div class="col-md-4">
                                                <label for="propertySelect">Property</label>
                                                <select class="form-control custom-select" id="propertySelect" name="property">
                                                    <option value="all" <%= selectedProperty === 'all' ? 'selected' : '' %>>All Properties</option>
                                                    <% properties.forEach(prop => { %>
                                                        <option value="<%= prop.name %>" <%= selectedProperty === prop.name ? 'selected' : '' %>><%= prop.name %></option>
                                                    <% }); %>
                                                </select>
                                            </div>
                                            <div class="col-md-4">
                                                <label>Date Range</label>
                                                <input type="date" class="form-control custom-input" id="reportDateFrom" name="dateFrom" value="<%= dateFrom %>">
                                                <input type="date" class="form-control custom-input mt-2" id="reportDateTo" name="dateTo" value="<%= dateTo %>">
                                            </div>
                                            <div class="col-md-4">
                                                <button type="submit" class="btn btn-primary btn-block"><i class="fas fa-file-export mr-2"></i> Generate Report</button>
                                            </div>
                                        </div>
                                    </form>
        
                                    <!-- Reports Table -->
                                    <div class="table-responsive">
                                        <table class="table table-striped table-hover custom-table">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Property</th>
                                                    <th>Occupancy</th>
                                                    <th>Rent Collected</th>
                                                    <th>Pending</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% reports.forEach((report, index) => { %>
                                                    <tr>
                                                        <td><%= index + 1 %></td>
                                                        <td><%= report.propertyName || 'Unnamed Property' %></td>
                                                        <td><%= report.occupancyRate ? Number(report.occupancyRate).toFixed(2) + '%' : 'N/A' %></td>
                                                        <td>Ksh.<%= Number(report.totalRentCollected || 0).toFixed(2) %></td>
                                                        <td>Ksh.<%= Number(report.pendingPayments || 0).toFixed(2) %></td>
                                                    </tr>
                                                <% }); %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
        
                        <!-- Invoices Section -->
                        <div class="tab-pane fade" id="invoices" role="tabpanel" aria-labelledby="invoices-tab">
                            <div class="card custom-card">
                                <div class="card-header">
                                    <h4><i class="fas fa-calculator mr-2"></i> Invoice Management</h4>
                                </div>
                                <div class="card-body">
                                    <!-- Filter Form -->
                                    <div class="row mb-4 align-items-end">
                                        <div class="col-md-4">
                                            <label>Date Range</label>
                                            <input type="date" class="form-control custom-input" id="invoiceDateFrom">
                                            <input type="date" class="form-control custom-input mt-2" id="invoiceDateTo">
                                        </div>
                                        <div class="col-md-4">
                                            <label for="invoiceStatus">Status</label>
                                            <select class="form-control custom-select" id="invoiceStatus">
                                                <option value="all">All</option>
                                                <option value="paid">Paid</option>
                                                <option value="pending">Pending</option>
                                                <option value="overdue">Overdue</option>
                                            </select>
                                        </div>
                                        <div class="col-md-4">
                                            <button class="btn btn-primary btn-block" id="filterInvoices"><i class="fas fa-filter mr-2"></i> Filter Invoices</button>
                                        </div>
                                    </div>
        
                                    <!-- Invoices Table -->
                                    <div class="table-responsive">
                                        <table class="table table-striped table-hover custom-table" id="invoicesTable">
                                            <thead>
                                                <tr>
                                                    <th>Invoice #</th>
                                                    <th>Transaction ID</th>
                                                    <th>Amount</th>
                                                    <th>Billing Period</th>
                                                    <th>Status</th>
                                                    <th>Due Date</th>
                                                    <th>Time</th>
                                                    <th>Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% invoices.forEach(invoice => { %>
                                                    <tr data-due-date="<%= invoice.dueDate %>" data-status="<%= invoice.status %>">
                                                        <td><%= invoice.invoiceNumber %></td>
                                                        <td><%= invoice.transactionId || 'N/A' %></td>
                                                        <td>Ksh.<%= invoice.amount.toFixed(2) %></td>
                                                        <td><%= invoice.billingPeriod.charAt(0).toUpperCase() + invoice.billingPeriod.slice(1) %></td>
                                                        <td>
                                                            <span class="badge <%= invoice.status === 'paid' ? 'badge-success' : invoice.status === 'overdue' ? 'badge-danger' : 'badge-warning' %>">
                                                                <%= invoice.status %>
                                                            </span>
                                                        </td>
                                                        <td><%= new Date(invoice.dueDate).toLocaleDateString() %></td>
                                                        <td><%= new Date(invoice.time).toLocaleString() %></td>
                                                        <td>
                                                            <% if (invoice.status === 'pending') { %>
                                                                <form action="/invoices/pay/<%= invoice._id %>" method="POST" style="display:inline;">
                                                                    <button class="btn btn-success btn-sm" type="submit"><i class="fas fa-money-check-alt mr-1"></i> Pay</button>
                                                                </form>
                                                            <% } %>
                                                            <a href="/invoices/download/<%= invoice._id %>" class="btn btn-info btn-sm">
                                                                <i class="fas fa-download"></i>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                <% }); %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section><!-- /.content -->
        
            <!-- Control Sidebar -->
            <aside class="control-sidebar control-sidebar-dark"></aside>
        
            <!-- Main Footer -->
            <footer class="main-footer">
                <strong>Copyright Â© 2024-2025 <a href="https://leasecaptain.com/">Lease Captain</a></strong>
                All rights reserved.
                <div class="float-right d-none d-sm-inline-block">
                    <b>Version</b> 1.0.2
                </div>
            </footer>
