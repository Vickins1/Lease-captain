        <div class="content-wrapper">
            <!-- Main Content -->
            <section class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h3 class="card-title">Tenant Report - <%= tenant.name %></h3>
                                    <div class="d-flex align-items-center">
                                        <button class="btn btn-primary mr-2" data-toggle="modal" 
                                                data-target="#editTenantModal<%= tenant._id %>">
                                            <i class="fas fa-edit"></i> Edit
                                        </button>
                                        <form action="/tenancy-manager/tenant/resend-email/<%= tenant._id %>" method="POST" class="d-inline mr-2">
                                            <button class="btn btn-info" type="submit">
                                                <i class="fas fa-envelope"></i> Resend Logins Email
                                            </button>
                                        </form>
                                        <button class="btn btn-danger" data-toggle="modal" 
                                                data-target="#deleteTenantModal<%= tenant._id %>">
                                            <i class="fas fa-trash-alt"></i> Delete
                                        </button>
                                        <!-- New Button for Direct Tenant Portal Access -->
                                        <form action="/tenancy-manager/tenant/impersonate/<%= tenant._id %>" method="POST" class="d-inline mr-2">
                                            <button class="btn btn-success" type="submit">
                                                <i class="fas fa-user"></i> Access Tenant Portal
                                            </button>
                                        </form>
                                        <a href="/tenancy-manager/tenants" class="btn-back ml-3"><i class="fas fa-arrow-left"></i> Back to Tenants</a>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <!-- Flash Messages -->
                                    <% if (error.length > 0) { %>
                                        <div class="alert alert-danger"><%= error %></div>
                                    <% } %>
                                    <% if (success.length > 0) { %>
                                        <div class="alert alert-success"><%= success %></div>
                                    <% } %>
        
                                    <!-- Tenant Details -->
                                    <div class="row mb-4">
                                        <div class="col-md-6">
                                            <h4>Tenant Information</h4>
                                            <p><strong>Name:</strong> <%= tenant.name %></p>
                                            <p><strong>Email:</strong> <%= tenant.email %></p>
                                            <p><strong>Phone:</strong> <%= tenant.phone %></p>
                                            <p><strong>Property:</strong> <%= tenant.property?.name || 'N/A' %></p>
                                            <p><strong>Address:</strong> <%= tenant.property?.address || 'N/A' %></p>
                                            <p><strong>Unit:</strong> <%= tenant.unit?.unitType || 'N/A' %> - Door <%= tenant.doorNumber %></p>
                                        </div>
                                        <div class="col-md-6">
                                            <h4>Lease Details</h4>
                                            <p><strong>Start Date:</strong> <%= leaseStart %></p>
                                            <p><strong>End Date:</strong> <%= leaseEnd %></p>
                                            <p><strong>Days Remaining:</strong> <%= daysRemaining %></p>
                                            <div class="progress">
                                                <div class="progress-bar bg-success" role="progressbar" 
                                                     data-progress="<%= leaseProgress %>" 
                                                     aria-valuenow="<%= leaseProgress %>" 
                                                     aria-valuemin="0" aria-valuemax="100">
                                                    <%= leaseProgress %>%
                                                </div>
                                            </div>
                                            <script>
                                                document.addEventListener("DOMContentLoaded", function() {
    // Get all progress bars with the data-progress attribute
    const progressBars = document.querySelectorAll('.progress-bar[data-progress]');
    
    progressBars.forEach(function(bar) {
        const progress = bar.getAttribute('data-progress'); // Get the progress value
        bar.style.width = progress + '%'; // Set the width dynamically
        bar.setAttribute('aria-valuenow', progress); // Update aria-valuenow for accessibility
        bar.textContent = progress + '%'; // Update the text inside the progress bar
    });
});

                                            </script>
                                        </div>
                                    </div>
        
                                    <!-- Summary Boxes -->
                                    <div class="row mb-4">
                                        <div class="col-md-3 col-sm-6">
                                            <div class="summary-box">
                                                <h5>Total Paid</h5>
                                                <p class="text-success">Ksh. <%= totalPaid %></p>
                                            </div>
                                        </div>
                                        <div class="col-md-3 col-sm-6">
                                            <div class="summary-box">
                                                <h5>Rent Due</h5>
                                                <p class="text-danger">Ksh. <%= rentDue %></p>
                                            </div>
                                        </div>
                                        <div class="col-md-3 col-sm-6">
                                            <div class="summary-box">
                                                <h5>Utility Due</h5>
                                                <p class="text-danger">Ksh. <%= utilityDue %></p>
                                            </div>
                                        </div>
                                        <div class="col-md-3 col-sm-6">
                                            <div class="summary-box">
                                                <h5>Overpayment</h5>
                                                <p class="text-info">Ksh. <%= overpayment %></p>
                                            </div>
                                        </div>
                                    </div>
        
                                    <!-- Payment History -->
                                    <h4>Payment History</h4>
                                    <div class="table-responsive">
                                        <table class="table table-bordered table-hover table-striped">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Transaction ID</th>
                                                    <th>Type</th>
                                                    <th>Amount</th>
                                                    <th>Date Paid</th>
                                                    <th>Method</th>
                                                    <th>Status</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% payments.forEach((payment, index) => { %>
                                                    <tr>
                                                        <td><%= index + 1 %></td>
                                                        <td><%= payment.transactionId || 'N/A' %></td>
                                                        <td><%= payment.paymentType || 'N/A' %></td>
                                                        <td>Ksh. <%= payment.amount ? payment.amount.toFixed(2) : '0.00' %></td>
                                                        <td><%= payment.datePaid ? new Date(payment.datePaid).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) : 'N/A' %></td>
                                                        <td><%= payment.method || 'N/A' %></td>
                                                        <td>
                                                            <span class="<%= payment.status === 'completed' ? 'text-success' : 
                                                                            payment.status === 'pending' ? 'text-warning' : 
                                                                            payment.status === 'failed' ? 'text-danger' : 'text-secondary' %>">
                                                                <i class="fas <%= payment.status === 'completed' ? 'fa-check-circle' : 
                                                                                 payment.status === 'pending' ? 'fa-hourglass-half' : 
                                                                                 payment.status === 'failed' ? 'fa-times-circle' : 'fa-question-circle' %>"></i>
                                                                <%= payment.status || 'N/A' %>
                                                            </span>
                                                        </td>
                                                    </tr>
                                                <% }); %>
                                                <% if (payments.length === 0) { %>
                                                    <tr>
                                                        <td colspan="7" class="text-center">No payment history available.</td>
                                                    </tr>
                                                <% } %>
                                            </tbody>
                                        </table>
                                    </div>
        
                                    <!-- Modals -->
                                    <!-- Edit Tenant Modal -->
                                    <div class="modal fade" id="editTenantModal<%= tenant._id %>" tabindex="-1" 
                                         role="dialog" aria-labelledby="editTenantModalLabel<%= tenant._id %>" aria-hidden="true">
                                        <div class="modal-dialog" role="document">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="editTenantModalLabel<%= tenant._id %>">Edit Tenant</h5>
                                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                                <form action="/tenancy-manager/tenants/edit/<%= tenant._id %>" method="POST">
                                                    <div class="modal-body">
                                                        <div class="form-group">
                                                            <label for="name<%= tenant._id %>">Name</label>
                                                            <input type="text" class="form-control" id="name<%= tenant._id %>" 
                                                                   name="name" value="<%= tenant.name %>" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="email<%= tenant._id %>">Email</label>
                                                            <input type="email" class="form-control" id="email<%= tenant._id %>" 
                                                                   name="email" value="<%= tenant.email %>" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="phone<%= tenant._id %>">Phone</label>
                                                            <input type="text" class="form-control" id="phone<%= tenant._id %>" 
                                                                   name="phone" value="<%= tenant.phone %>" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="leaseEndDate<%= tenant._id %>">Lease End Date</label>
                                                            <input type="date" class="form-control" id="leaseEndDate<%= tenant._id %>" 
                                                                   name="leaseEndDate" value="<%= new Date(tenant.leaseEndDate).toISOString().split('T')[0] %>" required>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="propertyId<%= tenant._id %>">Property</label>
                                                            <select class="form-control" id="propertyId<%= tenant._id %>" name="propertyId" required>
                                                                <% properties.forEach(property => { %>
                                                                    <option value="<%= property._id %>" 
                                                                            <%= property._id.toString() === (tenant.property?._id?.toString() || '') ? 'selected' : '' %>>
                                                                        <%= property.name %>
                                                                    </option>
                                                                <% }) %>
                                                            </select>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="rentPaid<%= tenant._id %>">Rent Paid</label>
                                                            <input type="number" class="form-control" id="rentPaid<%= tenant._id %>" 
                                                                   name="rentPaid" value="<%= tenant.rentPaid || 0 %>" step="0.01" min="0">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="utilityPaid<%= tenant._id %>">Utility Paid</label>
                                                            <input type="number" class="form-control" id="utilityPaid<%= tenant._id %>" 
                                                                   name="utilityPaid" value="<%= tenant.utilityPaid || 0 %>" step="0.01" min="0">
                                                        </div>
                                                    </div>
                                                    <div class="modal-footer">
