<script>
    document.addEventListener('DOMContentLoaded', () => {
        const searchInput = document.getElementById('searchInput');
        const searchForm = document.getElementById('searchForm');
        let debounceTimer;

        // Client-side filtering with debounce
        function filterExpenses(searchTerm) {
            clearTimeout(debounceTimer);
            debounceTimer = setTimeout(() => {
                const rows = document.querySelectorAll('#expensesTable tbody tr');
                const term = searchTerm.trim().toLowerCase();

                rows.forEach(row => {
                    const expenseName = row.children[1].textContent.toLowerCase();
                    const category = row.children[2].textContent.toLowerCase();
                    const shouldShow = expenseName.includes(term) || category.includes(term);
                    row.style.display = shouldShow ? '' : 'none';
                });
            }, 300);
        }

        // Event listeners
        if (searchInput) {
            searchInput.addEventListener('input', (e) => filterExpenses(e.target.value));
            
            // Preserve search term on page load
            const urlParams = new URLSearchParams(window.location.search);
            const searchTerm = urlParams.get('search');
            if (searchTerm) {
                searchInput.value = searchTerm;
                filterExpenses(searchTerm);
            }
        }

        // Form validation
        const expenseForm = document.getElementById('createExpenseForm');
        if (expenseForm) {
            expenseForm.addEventListener('submit', (e) => {
                const amount = document.getElementById('amount');
                if (amount.value < 0) {
                    e.preventDefault();
                    alert('Amount cannot be negative');
                    return false;
                }
            });
        }
    });
</script>

