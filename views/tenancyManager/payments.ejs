        <style>
            @media (max-width: 768px) {
                .form-control {
                    font-size: 10px;
                    padding: 5px;
                }

                .table th,
                .table td {
                    font-size: 10px;
                    padding: 8px;
                }

                .pagination .page-link {
                    font-size: 9px;
                }

                .table thead th {
                    padding: 10px;
                }
            }

            @media (max-width: 576px) {
                .table {
                    display: block;
                    overflow-x: auto;
                }

                .table th,
                .table td {
                    font-size: 10px;
                }

                .table-responsive {
                    overflow-x: auto;
                }

                .pagination .page-link {
                    font-size: 10px;
                }

                .form-control {
                    font-size: 10px;
                }
            }

            .table-container {
                margin-bottom: 1rem;
            }
        </style>

        <script>
            document.addEventListener('DOMContentLoaded', function () {
                const searchInput = document.getElementById('searchInput');

                searchInput.addEventListener('input', function () {
                    const query = searchInput.value;

                    // Perform an AJAX request or filter results

                    fetch(`/tenancy-manager/payments?search=${encodeURIComponent(query)}`)
                        .then(response => response.text())
                        .then(data => {
                            // Update your results section with the new data
                            document.querySelector('.table-container').innerHTML = data;
                        })
                        .catch(error => console.error('Error fetching data:', error));
                });
            });
        </script>


