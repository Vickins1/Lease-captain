        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h3 class="card-title">Transactions</h3>
                                </div>

                                <!-- Search Form -->
                                <div class="mb-3">
                                    <form id="searchForm" action="/tenancy-manager/payments" method="GET"
                                        class="d-flex flex-column flex-sm-row justify-content-center">
                                        <input type="text" id="searchInput" name="search"
                                            class="form-control me-2 mb-2 mb-sm-0"
                                            placeholder="Search by Tenant Name or Property" aria-label="Search">
                                    </form>
                                </div>



                                <!-- /.card-header -->
                                <div class="card-body">
                                    <table id="paymentsTable" class="table table-bordered table-hover table-striped">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Transaction ID</th>
                                                <th>Tenant Name</th>
                                                <th>Property</th>
                                                <th>Unit</th>
                                                <th>Door</th>
                                                <th>Payment Type</th>
                                                <th>Total Amount</th>
                                                <th>Date Paid</th>
                                                <th>Payment Method</th>
                                                <th>Status</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% payments.forEach((payment, index)=> { %>
                                                <tr>
                                                    <td>
                                                        <%= (currentPage - 1) * pageSize + index + 1 %>
                                                    </td>
                                                    <td>
                                                        <%= payment.transactionId || 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.tenant?.name || 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.tenant?.property?.name || 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.tenant?.unit?.unitType || 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.tenant?.doorNumber || 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.paymentType || 'N/A' %>
                                                    </td>
                                                    <td>Ksh. <%= payment.amount ? payment.amount.toFixed(2) : '0.00' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.datePaid ? new
                                                            Date(payment.datePaid).toLocaleDateString() : 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <%= payment.method || 'N/A' %>
                                                    </td>
                                                    <td>
                                                        <span class="<%= payment.status === 'completed' ? 'text-success' : 
                                    payment.status === 'pending' ? 'text-warning' : 
                                    payment.status === 'failed' ? 'text-danger' : 'text-secondary' %>">
                                                            <i class="fas <%= payment.status === 'completed' ? 'fa-check-circle' : 
                                         payment.status === 'pending' ? 'fa-hourglass-half' : 
                                         payment.status === 'failed' ? 'fa-times-circle' : 'fa-question-circle' %>">
                                                            </i>
                                                            <%= payment.status || 'N/A' %>
                                                        </span>
                                                    </td>
                                                </tr>
                                                <% }); %>
                                        </tbody>
                                       
                                    </table>
                                </div>
                                <!-- /.card-body -->
                            </div>
                            <!-- /.card -->
                        </div>
                        <!-- /.col -->
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.container-fluid -->
            </section>
            <!-- /.content -->


            <!-- Pagination -->
            <nav aria-label="Page navigation">
                <ul class="pagination justify-content-center">
                    <% if (currentPage> 1) { %>
                        <li class="page-item">
                            <a class="page-link" href="/tenancy-manager/payments?page=<%= currentPage - 1 %>"
                                aria-label="Previous">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        <% } %>
                            <% for (let i=1; i <=totalPages; i++) { %>
                                <li class="page-item <%= i === currentPage ? 'active' : '' %>">
                                    <a class="page-link" href="/tenancy-manager/payments?page=<%= i %>">
                                        <%= i %>
                                    </a>
                                </li>
                                <% } %>
                                    <% if (currentPage < totalPages) { %>
                                        <li class="page-item">
                                            <a class="page-link"
                                                href="/tenancy-manager/payments?page=<%= currentPage + 1 %>"
                                                aria-label="Next">
                                                <span aria-hidden="true">&raquo;</span>
                                            </a>
                                        </li>
                                        <% } %>
                </ul>
            </nav>
