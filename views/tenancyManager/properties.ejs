<div class="content-wrapper">
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card custom-card">
                        <div class="card-header">
                            <h3 class="card-title"><i class="fas fa-building mr-2"></i> Properties</h3>
                        </div>
                        <div class="card-body">
                            <!-- Messages -->
                            <% if (error && error.length > 0) { %>
                                <div class="alert alert-danger animate__animated animate__fadeIn">
                                    <i class="fas fa-exclamation-circle mr-2"></i> <%= error %>
                                </div>
                            <% } %>
                            <% if (success && success.length > 0) { %>
                                <div class="alert alert-success animate__animated animate__fadeIn">
                                    <i class="fas fa-check-circle mr-2"></i> <%= success %>
                                </div>
                            <% } %>

                            <!-- Add Property Button -->
                            <div class="text-right mb-4">
                                <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#propertyModal">
                                    <i class="fas fa-plus mr-2"></i> Add Property
                                </button>
                            </div>

                            <!-- Properties Table -->
                            <% if (properties && properties.length > 0) { %>
                                <div class="table-responsive">
                                    <table id="propertiesTable" class="table table-striped table-hover custom-table">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Name</th>
                                                <th>Address</th>
                                                <th>Units</th>
                                                <th>Tenants</th>
                                                <th>Pay Day</th>
                                                <th>Type</th>
                                                <th>Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% properties.forEach((property, index) => { %>
                                                <tr>
                                                    <td><%= index + 1 + (currentPage - 1) * 10 %></td>
                                                    <td class="text-truncate"><%= property.name %></td>
                                                    <td class="text-truncate"><%= property.address %></td>
                                                    <td><%= property.numberOfUnits %></td>
                                                    <td><%= property.numberOfTenants %></td>
                                                    <td><%= property.paymentDay %></td>
                                                    <td><%= property.propertyType %></td>
                                                    <td>
                                                        <div class="btn-group" role="group">
                                                            <button class="btn btn-info btn-sm" data-toggle="modal" data-target="#editModal<%= property._id %>">
                                                                <i class="fas fa-edit"></i>
                                                            </button>
                                                            <button class="btn btn-danger btn-sm" data-toggle="modal" data-target="#deleteModal<%= property._id %>">
                                                                <i class="fas fa-trash-alt"></i>
                                                            </button>
                                                        </div>
                                                    </td>
                                                </tr>

                                                <!-- Edit Modal -->
                                                <div class="modal fade" id="editModal<%= property._id %>" tabindex="-1" role="dialog" aria-labelledby="editModalLabel<%= property._id %>" aria-hidden="true">
                                                    <div class="modal-dialog modal-lg" role="document">
                                                        <div class="modal-content custom-modal">
                                                            <div class="modal-header">
                                                                <h5 class="modal-title" id="editModalLabel<%= property._id %>"><i class="fas fa-edit mr-2"></i> Edit Property</h5>
                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                    <span aria-hidden="true">×</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                <form action="/tenancy-manager/property/edit/<%= property._id %>" method="POST" class="modern-form">
                                                                    <div class="form-group">
                                                                        <label for="propertyName<%= property._id %>">Name</label>
                                                                        <input type="text" class="form-control custom-input" id="propertyName<%= property._id %>" name="name" value="<%= property.name %>" required>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="propertyAddress<%= property._id %>">Address</label>
                                                                        <input type="text" class="form-control custom-input" id="propertyAddress<%= property._id %>" name="address" value="<%= property.address %>" required>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="paymentDay<%= property._id %>">Rent Payment Day</label>
                                                                        <input type="number" class="form-control custom-input" id="paymentDay<%= property._id %>" name="paymentDay" min="1" max="31" value="<%= property.paymentDay %>" required>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="propertyType<%= property._id %>">Property Type</label>
                                                                        <input type="text" class="form-control custom-input" id="propertyType<%= property._id %>" name="propertyType" value="<%= property.propertyType %>" placeholder="e.g., Studio Apartment" required>
                                                                    </div>
                                                                    <button type="submit" class="btn btn-primary btn-block"><i class="fas fa-save mr-2"></i> Save Changes</button>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <!-- Delete Modal -->
                                                <div class="modal fade" id="deleteModal<%= property._id %>" tabindex="-1" role="dialog" aria-labelledby="deleteModalLabel<%= property._id %>" aria-hidden="true">
                                                    <div class="modal-dialog" role="document">
                                                        <div class="modal-content custom-modal">
                                                            <div class="modal-header bg-danger text-white">
                                                                <h5 class="modal-title" id="deleteModalLabel<%= property._id %>"><i class="fas fa-trash-alt mr-2"></i> Delete Property</h5>
                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                    <span aria-hidden="true">×</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                Are you sure you want to delete "<%= property.name %>"?
                                                            </div>
                                                            <div class="modal-footer">
                                                                <form action="/tenancy-manager/property/delete/<%= property._id %>" method="POST">
                                                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                                                                    <button type="submit" class="btn btn-danger">Delete</button>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            <% }); %>
                                        </tbody>
                                    </table>
                                </div>
                            <% } else { %>
                                <div class="alert alert-warning text-center animate__animated animate__fadeIn">
                                    <i class="fas fa-exclamation-triangle mr-2"></i> No properties found.
                                </div>
                            <% } %>

                            <!-- Pagination -->
                            <nav aria-label="Page navigation" class="mt-4">
                                <ul class="pagination justify-content-center custom-pagination">
                                    <% if (currentPage > 1) { %>
                                        <li class="page-item">
                                            <a class="page-link" href="/tenancy-manager/properties?page=<%= currentPage - 1 %>" aria-label="Previous">
                                                <i class="fas fa-arrow-left"></i>
                                            </a>
                                        </li>
                                    <% } %>
                                    <% for (let i = 1; i <= totalPages; i++) { %>
                                        <li class="page-item <%= i === currentPage ? 'active' : '' %>">
                                            <a class="page-link" href="/tenancy-manager/properties?page=<%= i %>"><%= i %></a>
                                        </li>
                                    <% } %>
                                    <% if (currentPage < totalPages) { %>
                                        <li class="page-item">
                                            <a class="page-link" href="/tenancy-manager/properties?page=<%= currentPage + 1 %>" aria-label="Next">
                                                <i class="fas fa-arrow-right"></i>
                                            </a>
                                        </li>
                                    <% } %>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Add Property Modal -->
    <div class="modal fade" id="propertyModal" tabindex="-1" role="dialog" aria-labelledby="propertyModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content custom-modal">
                <div class="modal-header">
                    <h5 class="modal-title" id="propertyModalLabel"><i class="fas fa-building mr-2"></i> Add Property</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">
                    <% if (error && error.length > 0) { %>
                        <div class="alert alert-danger animate__animated animate__fadeIn">
                            <i class="fas fa-exclamation-circle mr-2"></i> <%= error %>
                        </div>
                    <% } %>
                    <form action="/tenancy-manager/property" method="POST" class="modern-form">
                        <div class="form-group">
                            <label for="name">Property Name</label>
                            <input type="text" name="name" id="name" class="form-control custom-input" placeholder="Enter property name" required>
                        </div>
                        <div class="form-group">
                            <label for="address">Address</label>
                            <input type="text" name="address" id="address" class="form-control custom-input" placeholder="Enter property address" required>
                        </div>
                        <div class="form-group">
                            <label for="paymentDay">Rent Payment Day</label>
                            <input type="number" name="paymentDay" id="paymentDay" class="form-control custom-input" placeholder="Enter a day (1 - 31)" min="1" max="31" required>
                        </div>
                        <div class="form-group">
                            <label for="propertyType">Property Type</label>
                            <input type="text" name="propertyType" id="propertyType" class="form-control custom-input" placeholder="e.g., Apartment" required>
                        </div>
                        <button type="submit" class="btn btn-primary btn-block"><i class="fas fa-plus mr-2"></i> Add Property</button>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark"></aside>

    <!-- Main Footer -->
    <footer class="main-footer">
        <strong>Copyright © 2024-2025 <a href="https://leasecaptain.com/">Lease Captain</a></strong>
        All rights reserved.
        <div class="float-right d-none d-sm-inline-block">
            <b>Version</b> 1.0.2
