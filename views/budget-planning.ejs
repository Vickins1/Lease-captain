        <div class="content-wrapper">
              <div class="content-header">
                  <div class="container-fluid">
                      <div class="row mb-2">
                          <div class="col-sm-6">
                              <h1 class="m-0">Budget Planning</h1>
                              <p class="text-muted">Plan and track your property management finances</p>
                          </div>
                          <div class="col-sm-6">
                              <button class="btn btn-success float-right" data-toggle="modal" data-target="#newBudgetModal">
                                  <i class="fas fa-plus"></i> Add Budget Item
                              </button>
                          </div>
                      </div>
                  </div>
              </div>
  
              <section class="content">
                  <div class="container-fluid">
                      <!-- Financial Overview Cards -->
                      <div class="row">
                          <div class="col-md-3">
                              <div class="info-box bg-gradient-info budget-card">
                                  <span class="info-box-icon"><i class="fas fa-file-contract"></i></span>
                                  <div class="info-box-content">
                                      <span class="info-box-text">Lease Revenue</span>
                                      <span class="info-box-number">$<%= leaseRevenue.toLocaleString() %></span>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3">
                              <div class="info-box bg-gradient-success budget-card">
                                  <span class="info-box-icon"><i class="fas fa-map"></i></span>
                                  <div class="info-box-content">
                                      <span class="info-box-text">Land Revenue</span>
                                      <span class="info-box-number">$<%= landRevenue.toLocaleString() %></span>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3">
                              <div class="info-box bg-gradient-warning budget-card">
                                  <span class="info-box-icon"><i class="fas fa-car"></i></span>
                                  <div class="info-box-content">
                                      <span class="info-box-text">Parking Fees</span>
                                      <span class="info-box-number">$<%= parkingFees.toLocaleString() %></span>
                                  </div>
                              </div>
                          </div>
                          <div class="col-md-3">
                              <div class="info-box bg-gradient-danger budget-card">
                                  <span class="info-box-icon"><i class="fas fa-users"></i></span>
                                  <div class="info-box-content">
                                      <span class="info-box-text">Staff Expenses</span>
                                      <span class="info-box-number">$<%= staffExpenses.toLocaleString() %></span>
                                  </div>
                              </div>
                          </div>
                      </div>
  
                      <!-- Budget vs Actual Chart -->
                      <div class="row">
                          <div class="col-md-12">
                              <div class="card">
                                  <div class="card-header">
                                      <h3 class="card-title">Budget vs Actual Spending</h3>
                                  </div>
                                  <div class="card-body">
                                      <canvas id="budgetChart" style="min-height: 300px;"></canvas>
                                  </div>
                              </div>
                          </div>
                      </div>
  
                      <!-- Budget Planner Table -->
                      <div class="card budget-planner">
                          <div class="card-header">
                              <h3 class="card-title">Budget Planner</h3>
                              <div class="card-tools">
                                  <button class="btn btn-sm btn-info"><i class="fas fa-download"></i> Export</button>
                              </div>
                          </div>
                          <div class="card-body p-0">
                              <table class="table table-striped table-hover">
                                  <thead>
                                      <tr>
                                          <th>Category</th>
                                          <th>Amount</th>
                                          <th>Period</th>
                                          <th>Description</th>
                                      </tr>
                                  </thead>
                                  <tbody>
                                      <% budgets.forEach(budget => { %>
                                          <tr>
                                              <td><%= budget.category %></td>
                                              <td>$<%= budget.amount.toLocaleString() %></td>
                                              <td><%= budget.period %></td>
                                              <td><%= budget.description || 'N/A' %></td>
                                              
                                          </tr>
                                      <% }) %>
                                  </tbody>
                              </table>
                          </div>
                      </div>
                  </div>
              </section>
          </div>
  
          <!-- New Budget Modal -->
          <div class="modal fade" id="newBudgetModal">
              <div class="modal-dialog modal-lg">
                  <div class="modal-content">
                      <div class="modal-header bg-gradient-success">
                          <h4 class="modal-title">Add Budget Item</h4>
                          <button type="button" class="close" data-dismiss="modal">×</button>
                      </div>
                      <div class="modal-body">
                          <form action="/budget-planning" method="POST">
                              <div class="form-group">
                                  <label>Category</label>
                                  <select name="category" class="form-control" required>
                                      <option value="">Select Category</option>
                                      <option value="Leases">Leases</option>
                                      <option value="Properties">Properties</option>
                                      <option value="Lands">Lands</option>
                                      <option value="Vehicles">Vehicles</option>
                                      <option value="Staff">Staff</option>
                                      <option value="Miscellaneous">Miscellaneous</option>
                                  </select>
                              </div>
                              <div class="form-group">
                                  <label>Amount</label>
                                  <input type="number" name="amount" class="form-control" required>
                              </div>
                              <div class="form-group">
                                  <label>Period</label>
                                  <input type="text" name="period" class="form-control" placeholder="e.g., 2025-04" required>
                              </div>
                              <div class="form-group">
                                  <label>Description (Optional)</label>
                                  <textarea name="description" class="form-control" rows="3"></textarea>
                              </div>
                              <button type="submit" class="btn btn-success">Save Budget</button>
                          </form>
                      </div>
                  </div>
              </div>
          </div>
        
